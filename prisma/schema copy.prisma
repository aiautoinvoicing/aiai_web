generator client_mysql {
    provider = "prisma-client-js"
    output   = "../../prisma/generated/mysql-client"
}

datasource mysql {
    provider = "mysql"
    url      = env("MYSQL_DATABASE_URL")
}

model Invoice {
    id          String   @id @db.VarChar(36)
    customer_id String   @db.VarChar(36)
    amount      Int
    status      String   @db.VarChar(255)
    date        DateTime
}
